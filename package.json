{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:check": "echo '🚀 로컬 개발 서버 확인 중...' && curl -s http://localhost:8080 > /dev/null && echo '✅ 로컬 서버 정상 작동 (http://localhost:8080)' || echo '❌ 로컬 서버가 실행되지 않음. npm run dev를 먼저 실행하세요.'",
    "dev:open": "open http://localhost:8080",
    "dev:start": "npm run dev",
    "build": "tsc && vite build",
    "build:dev": "cp .env.development .env && tsc && vite build",
    "build:prod": "cp .env.production .env && tsc && vite build",
    "build:check": "npm run build && echo '✅ 빌드 완료! dist/ 폴더를 확인하세요.'",
    "preview": "vite preview",
    "preview:check": "npm run preview & sleep 3 && curl -s http://localhost:4173 > /dev/null && echo '✅ 프리뷰 서버 정상 작동 (http://localhost:4173)' || echo '❌ 프리뷰 서버 오류'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "echo '테스트가 설정되지 않았습니다.'",
    "deploy:build": "./scripts/build-production.sh",
    "deploy:dev": "./scripts/deploy-dev.sh",
    "deploy:dev:build": "npm run deploy:build && ./scripts/deploy-dev.sh",
    "deploy:prod": "./scripts/deploy-aws.sh production",
    "deploy:prod:build": "npm run deploy:build && ./scripts/deploy-aws.sh production",
    "deploy:check": "./scripts/local-check.sh",
    "deploy:preview": "npm run build && npm run preview",
    "deploy:preview:open": "npm run deploy:preview & sleep 3 && open http://localhost:4173",
    "check": "./scripts/local-check.sh",
    "gitflow:feature": "./scripts/gitflow.sh feature-start",
    "gitflow:feature-finish": "./scripts/gitflow.sh feature-finish",
    "gitflow:release": "./scripts/gitflow.sh release-start",
    "gitflow:release-finish": "./scripts/gitflow.sh release-finish",
    "gitflow:hotfix": "./scripts/gitflow.sh hotfix-start",
    "gitflow:hotfix-finish": "./scripts/gitflow.sh hotfix-finish",
    "gitflow:status": "./scripts/gitflow.sh status",
    "gitflow:help": "./scripts/gitflow.sh help",
    "env:dev": "cp env.development.example .env.development && echo '✅ 개발환경 설정 파일 생성됨: .env.development'",
    "env:local": "cp env.local.example .env.local && echo '✅ 로컬환경 설정 파일 생성됨: .env.local'",
    "env:prod": "cp env.production.example .env.production && echo '✅ 운영환경 설정 파일 생성됨: .env.production'",
    "env:check": "echo '🔍 환경변수 파일 확인:' && ls -la .env* 2>/dev/null || echo '❌ 환경변수 파일이 없습니다'",
    "db:setup": "./scripts/setup-dev-database-complete.sql && echo '✅ 개발 DB 설정 완료'",
    "db:sync": "./scripts/sync-dev-db.sh",
    "db:migrate": "./scripts/migrate-schema.sh",
    "db:extract": "echo '📋 운영 DB 스키마 추출:' && echo '1. 운영 Supabase Dashboard 접속' && echo '2. SQL Editor에서 scripts/export-production-schema.sql 실행' && echo '3. 결과를 sql/prod_schema.sql로 저장'",
    "db:apply": "echo '📋 개발 DB 스키마 적용:' && echo '1. 개발 Supabase Dashboard 접속' && echo '2. SQL Editor에서 scripts/setup-dev-database-complete.sql 실행' && echo '3. 또는 sql/prod_schema.sql 실행'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.848.0",
    "@aws-sdk/lib-storage": "^3.848.0",
    "@aws-sdk/s3-request-presigner": "^3.848.0",
    "@hookform/resolvers": "^3.9.0",
